. /lib/functions/uci-defaults.sh

board_config_update

case "$(board_name)" in
sifive,hifive-unleashed-a00)
	ucidef_set_led_netdev "lan" "LAN" "green:d3" "eth0"
	;;
sifive,hifive-unmatched-a00)
	ucidef_set_led_netdev "lan" "LAN" "green:d2" "eth0"
	;;
esac

board_config_flush

exit 0
